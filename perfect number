import java.util.Scanner;
public class Perfectnumber{
    public static void main(String[] args){
    Scanner in = new Scanner(System.in);
    System.out.println("enter the number for perfectnumber checking:");
    int n = in.nextInt();
    int sum=0;
    for(int i=1;i<n;i++){
       if(n%i==0){
        sum+=i;
       }
   }
   if(sum==n){
              System.out.println( n+" perfectnumber");
      }
   else{
      System.out.println( n+" not a perfectnumber");
   }
   }
}
    
